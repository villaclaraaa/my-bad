{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DB_CONNECTION_STRING"
  },
  "BotSettings": {
    "Tg_BotToken": "YOUR_TELEGRAM_BOT_CLIENT_TOKEN",
    "Tg_BotWebhookUrl": "YOUR_TELEGRAM_BOT_SET_WEBHOOK_URL/tgbot"
  },
  "ApiKeys": {
    "XApiAccess": "YOUR_SECRET_API_KEY"
  },

  // Environmental variables are used when running in containers.
  // This is actually the same values as above, but it only shows syntax, where you should put '=' instead of ':'.
  "EnvVariables": {
    "PostgressUser": "POSTGRES_USER=YOUR_USERNAME",
    "PostgressPassword": "POSTGRES_PASSWORD=YOUR_PASSWORD",
    "PostgressDb": "POSTGRES_DB=YOUR_DB",

    "BotSettingsTgToken": "BotSettings_Tg_BotToken=YOUR_TOKEN",
    "BotSettingsTgWebhook": "BotSettings_Tg_BotWebhookUrl=YOUR_WEBHOOK/tgbot",
    "ApiKeyXApiAccess": "ApiKeys_XApiAccess=YOUR_SECRET_API_KEY",
    "SupabaseConnstring": "Supabase=YOUR_DB_CONNECTION_STRING"
  }
}